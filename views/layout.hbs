                    <img id="btnLlamar" class="w3-button w3-round imgBtnEncabezado" src="/images/imgPlanteamientoLlamar.png" usemap="#mapBtnDesplegarCaptura" style="display: none" title="Llamar a planteamiento">
                    <map id="mapBtnLlamar" name="mapBtnLlamar"></map>
                </th>
                <th id="thOcultarOpcionesSesion" class="tblSangriaDerecha w3-round">
                    <img id="btnOcultarOpcionesSesion" class="w3-button w3-round imgBtnEncabezado" src="/images/imgOcultar.png" style="display: none" title="Ocultar opciones">
                </th>
                <th id="thDesplegarOpcionesSesion" class="tblSangriaDerecha w3-round">
                    <img id="btnDesplegarOpcionesSesion" class="w3-button w3-round imgBtnEncabezado" src="/images/imgDesplegar.png" usemap="#mapBtnDesplegarCaptura" style="display: inline-block" title="Desplegar opciones">
                    <label id="lblOpcionesSesion" class="lblSesionResaltado" style="display: none"></label>
                </th>
            </tr>
        </table>
    </div>
    <div id="divListado" class="divSesion" align="left">
        <table class="tblSesion">
            <tr>
                <th id="thSangriaIzquierdaListado" class="tblSangriaIzquierda w3-round">
                    <img id="imgBtnNada" class="w3-button w3-round imgBtnEncabezado" src="/images/imgNada.png" style="display: inline-block">
                </th>
                <th id="thCursos" class="tblListado w3-round">
                    <img id="btnDesplegarCursos" class="w3-button w3-round imgBtnEncabezado" src="/images/imgCurso.png" style="display: inline-block">
                    <a id="lblCursos" class="lblSesion">Cursos</a>
                </th>
                <th id="thGrupos" class="tblListado w3-round">
                    <img id="btnDesplegarGrupos" class="w3-button w3-round imgBtnEncabezado" src="/images/imgCurso.png" style="display: inline-block">
                    <a id="lblGrupos" class="lblSesion">Grupos</a>
                </th>
                <th id="thDocentes" class="tblListado w3-round">
                    <img id="btnDesplegarDocentes" class="w3-button w3-round imgBtnEncabezado" src="/images/imgDocente.png" style="display: inline-block">
                    <a id="lblDocentes" class="lblSesion">Docentes</a>
                </th>
                <th id="thEstudiantes" class="tblListado w3-round">
                    <img id="btnDesplegarEstudiantes" class="w3-button w3-round imgBtnEncabezado" src="/images/imgEstudiante.png" style="display: inline-block">
