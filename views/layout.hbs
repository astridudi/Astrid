    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
</head>
<body>
<header>
    <div style="display: none">
        <b id="lblAplicacion" class="bEncabezado">Astrid 1.0</b>
        <b id="urlServidor" style="display: none"></b>
        <b id="alerta" ></b>
        <b id="banderaReserva" ></b>
    </div>
    <div id="divEncabezado" class="divEncabezado">
        <table class="tblEncabezado">
            <tr>
                <th>
                    <div id="divAplicacion" style="display: flex; flex-direction: row;" align="left">
                        <img id="imgBtnInicio" class="w3-button w3-round imgBtnEncabezado" src="/images/imgLogo.png" usemap="#mapBtnInicio" title="Astrid 1.0">
                        <map id="mapBtnInicio" name="mapBtnInicio"></map>
                        <b id="lblUbicacion" class="bEncabezado"></b>
                    </div>
                </th>
                <th>
                    <div id="divNavegacion" style="display: flex; flex-direction: row-reverse;">
                        <img id="imgBtnSalir" class="w3-button w3-round imgBtnEncabezado" src="/images/imgSalir.png" usemap="#mapBtnSalir">
                        <img id="imgBtnCreditos" class="w3-button w3-round imgBtnEncabezado" src="/images/imgLogo.png" usemap="#mapBtnCreditos">
                        <img id="imgBtnEstadisticas" class="w3-button w3-round imgBtnEncabezado" src="/images/imgLogo.png" usemap="#mapBtnEstadisticas">
                        <img id="imgBtnSesiones" class="w3-button w3-round imgBtnEncabezado" src="/images/imgSesiones.png" usemap="#mapBtnSesiones">
                        <img id="imgBtnCasos" class="w3-button w3-round imgBtnEncabezado" src="/images/imgSesiones.png" usemap="#mapBtnCasos">
                        <img id="imgBtnInstituciones" class="w3-button w3-round imgBtnEncabezado" src="/images/imgInstituciones.png" usemap="#mapBtnInstituciones">
                        <img id="imgBtnIngresar" class="w3-button w3-round imgBtnEncabezado" src="/images/imgIngresar.png" usemap="#mapBtnIngresar" >
                        <div id="divSeparadorUsuario" style="width: 20px"></div>
                        <b id="lblUsuario" class="bEncabezado"></b>
                        <map id="mapBtnSalir" name="mapBtnSalir"></map>
                        <map id="mapBtnCreditos" name="mapBtnCreditos"></map>
                        <map id="mapBtnEstadisticas" name="mapBtnEstadisticas"></map>
                        <map id="mapBtnSesiones" name="mapBtnSesiones"></map>
                        <map id="mapBtnCasos" name="mapBtnCasos"></map>
                        <map id="mapBtnInstituciones" name="mapBtnInstituciones"></map>
                        <map id="mapBtnIngresar" name="mapBtnIngresar"></map>
                    </div>
                </th>
            </tr>
        </table>
    </div>
    <div id="divSeparadorEncabezado" class="divSeparadorEncabezado"></div>
    <div id="divSesion" class="divSesion" align="left">
